{{#with model}}
<h1>{{model.title}}</h1>
<main>
    <form id="note-input" >
        <input type="text" name="note"
               placeholder="Enter a new note here!"/>
        <button type="submit">Save</button>
    </form>
    <section>
        {{#each notes}}
        <div>
            <span>{{this}}</span>
        </div>
        {{/each}}
    </section>
</main>
{{/with}}